  "description": "Get any video or iframe card supported by https://noembed.com/#supported-sites. (Vimeo, YouTube, Facebook, Twitter, Instagram, etc)",
  "main": "index.js",
  "engines": {
    "gitbook": ">2.0.0"
  },
  "author": "thecryptodiver <thecryptodiver@protonmail.com> (http://thecryptodiver.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thecryptodiver/gitbook-plugin-noembed.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "gitbook",
    "gitbook-plugin",
    "videos",
    "oembed",
    "iframe",
    "noembed",
    "vimeo",
    "youtube",
    "instagram",
    "facebook"
  ],
  "gitbook": {
    "properties": {
      "thecryptodiver-embed": {
        "type": "string",
        "default": "it's the default value",
        "description": "It defines my awesome config!" 
      } 
    } 
  }
